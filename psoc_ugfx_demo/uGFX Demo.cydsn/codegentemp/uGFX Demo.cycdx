  </block>
  <block name="LCD_MISO" BASE="0x0" SIZE="0x0" desc="" visible="true" hidden="false" />
  <block name="ZeroTerminal_1" BASE="0x0" SIZE="0x0" desc="" visible="true" hidden="false" />
  <block name="LCD_MOSI" BASE="0x0" SIZE="0x0" desc="" visible="true" hidden="false" />
</blockRegMap>